{
    "version": 2,
    "builds": [
      {
        "src": "packages/ltbot/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/packages/ltbot/$1"
      }
    ]
  }